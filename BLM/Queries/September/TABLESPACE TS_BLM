CREATE TABLESPACE TS_BLM
LOGGING
DATAFILE '/DBF1/BLM_TS_BLM_01.DBF'
SIZE 32M
AUTOEXTEND ON
NEXT 32M MAXSIZE 2048M
EXTENT MANAGEMENT LOCAL
SEGMENT SPACE MANAGEMENT AUTO;


CREATE TEMPORARY TABLESPACE TEMP_TS_BLM
LOGGING
TEMPFILE '/DBF1/BLM_TEMP_TS_BLM_01.DBF'
SIZE 32M
AUTOEXTEND ON
NEXT 32M MAXSIZE 2048M
EXTENT MANAGEMENT LOCAL;

